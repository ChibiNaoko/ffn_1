<%= form_for [:admin, @season] do |f| %>
  <%= render "shared/error_messages", object: @season %>

  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>

  <%= f.label :description %>
  <%= f.text_field :description, class: "form-control" %>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
