<div class="col-sm-4">
  <div class="frg-result-rank">
    <div class="frg-tabs">
      <ul>
        <li><%= link_to t"home.result" %></li>
        <li><%= link_to t"home.ranking" %></li>
      </ul>
      <div id="frg-rank">
        <table class="table table-hover table-bordered">
          <thead>
            <tr>
              <th colspan="2">
                <%= select_tag "season_id", options_for_select(@seasons
                  .collect {|season| [season.name, season.id]}),
                  {class: "form-control"} %>
              </th>
            </tr>
          </thead>
          <tbody>
            <% @seasons.each do |season| %>
              <% season.load_season_team.each do |season_team| %>
                <tr>
                  <td><%= season_team.rank %></td>
                  <td><%= season_team.team_name %></td>
                </tr>
              <% end %>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
